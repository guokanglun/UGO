/* pages/goods_list/goods_list.wxss */


.goosList navigator{
    display: flex;
}
.goosList navigator .left{
    flex:2;
}
.goosList .left image{
    width: 80%;
    padding: 0 20rpx;
}
.goosList .right{
    flex: 3;
    display: flex;
    flex-direction: column;
    justify-content: space-around;
}

.goosList .right .title{
    display:-webkit-box;
    -webkit-box-orient:vertical;
    -webkit-line-clamp:2;
    overflow:hidden;
}
.goosList .right .price{
    color: #eb4450
}
